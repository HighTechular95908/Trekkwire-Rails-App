<% if current_account.customer_type == "traveler" %>
  <%= render partial: 'dashboard/traveler_dashboard' %>
<% else %>
  <h1 class="mt-3 text-3xl font-extrabold tracking-tight text-slate-900"><%= t(".title_guide") %></h1>
  <% unless current_account.approved %>
    <%= badge t(".not_approved"), class: "bg-red-100 text-red-800 text-xs font-medium" %>
    <a href = "mailto: jon@trekkwire.com" class="rounded bg-indigo-50 px-2 py-1 text-xs font-semibold text-indigo-600 shadow-sm hover:bg-indigo-100">Email Support</a>
  <% end %>
  <% unless current_account.active %>
    <div class="my-2">
      <%= badge t(".not_active"), class: "bg-red-100 text-red-800 text-xs font-medium" %>
      <%= link_to "Activate account", edit_account_path(current_account), class: "rounded bg-indigo-50 px-2 py-1 text-xs font-semibold text-indigo-600 shadow-sm hover:bg-indigo-100"%>
    </div>
  <% end %>
  <div class="my-2"><%= badge "Current Account: " + current_account.name %></div>
  <div class="my-10">
    <%= render partial: 'dashboard/guide_dashboard' %>
  </div>
<% end %>